[user]
	name = Jimmy Zhu
	email = jimmzhu@gmail.com
[alias]
	co = checkout
	br = branch
	st = status
	hi = "log --pretty=format:\"%C(auto)%h%d%n%s %C(#00a0ff)[%an %ad]\" --graph --date=short"
	hist = "log --pretty=format:\"%C(auto)%h%d%n%s %C(#00a0ff)[%an %ad]\" --graph --date=short"
[push]
	default = upstream
[core]
	excludesfile = ~/.gitignore
	editor = nvim
